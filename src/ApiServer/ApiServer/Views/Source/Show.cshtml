@model WomPlatform.Web.Api.DatabaseModels.Source

<html>
    <head>
        <title>Source @Model.Id</title>
    </head>

    <body>
        <h1>Source #@Model.Id @Model.Name</h1>
        <p><a href="@Model.Url">@Model.Url</a></p>
        <p>Contact: @Model.Contact.Name @Model.Contact.Surname @Model.Contact.Email</p>
    </body>
</html>
